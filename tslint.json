{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "jsRules": {
      "no-unused-expression": true
    },
    "rules": {
        "object-literal-sort-keys": false,
        "interface-name": false,
        "no-empty": false,
        "no-shadowed-variable": false,
        "no-console": false
    },
    "rulesDirectory": []
}
